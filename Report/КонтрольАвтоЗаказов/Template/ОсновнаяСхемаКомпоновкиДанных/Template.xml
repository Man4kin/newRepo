		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	АвтоЗаказСостав.Ссылка.Поставщик КАК Поставщик,
	АвтоЗаказСостав.Ссылка.РезультирующийДокумент КАК РезультирующийДокумент,
	АвтоЗаказСостав.Номенклатура,
	АвтоЗаказСостав.КоличествоРасчетное,
	АвтоЗаказСостав.Количество КАК КоличествоУтвержденное,
	ПланируемаяПоставкаСостав.Количество КАК КоличествоЗаказанное,
	АвтоЗаказСостав.Ссылка.Дата
ИЗ
	БизнесПроцесс.АвтоЗаказ.Состав КАК АвтоЗаказСостав
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ПланируемаяПоставка.Состав КАК ПланируемаяПоставкаСостав
		ПО АвтоЗаказСостав.Номенклатура = ПланируемаяПоставкаСостав.Номенклатура
			И АвтоЗаказСостав.Ссылка.РезультирующийДокумент = ПланируемаяПоставкаСостав.Ссылка
ГДЕ
	(АвтоЗаказСостав.Количество &gt; 0
			ИЛИ АвтоЗаказСостав.КоличествоРасчетное &gt; 0)
{ГДЕ
	(АвтоЗаказСостав.Ссылка.Дата &gt;= &amp;НачалоПериода),
	(АвтоЗаказСостав.Ссылка.Дата &lt;= &amp;КонецПериода)}</query>
