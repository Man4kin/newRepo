		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	sms_Store.StoreCode,
	100000 * sms_Store.StoreCode + sms_POS.PosCode КАК PosCode,
	sms_Employee.EmployeeCode,
	sms_Cheque.OpenTime КАК Дата,
	ВЫБОР
		КОГДА sms_Cheque.Amount &lt; 0
			ТОГДА sms_Cheque.Amount - &amp;Периодичность
		ИНАЧЕ sms_Cheque.Amount
	КОНЕЦ / &amp;Периодичность КАК IntervalSum,
	sms_Cheque.Amount КАК Сумма,
	sms_Cheque.Amount + sms_Cheque.DiscountValue КАК СуммаБезСкидки,
	sms_Cheque.DiscountValue КАК СуммаСкидки,
	sms_Cheque.Amount КАК МаксСуммаЧека,
	sms_Cheque.Amount КАК МинСуммаЧека,
	sms_Cheque.Amount КАК СрСуммаЧека,
	cl.ChequeLineCount КАК КоличествоПозиций,
	cl.ChequeLineCount КАК МаксКоличествоПозиций,
	cl.ChequeLineCount КАК МинКоличествоПозиций,
	cl.ChequeLineCount КАК СрЧекКоличествоПозиций,
	1 КАК КоличествоЧеков,
	ВЫБОР ДЕНЬНЕДЕЛИ(&amp;ПервоеЧисло2001)
		КОГДА 2
			ТОГДА ВЫБОР
					КОГДА ДЕНЬНЕДЕЛИ(sms_Cheque.CloseTime) = 1
						ТОГДА 7
					ИНАЧЕ ДЕНЬНЕДЕЛИ(sms_Cheque.CloseTime) - 1
				КОНЕЦ
		ИНАЧЕ ДЕНЬНЕДЕЛИ(sms_Cheque.CloseTime)
	КОНЕЦ КАК ДеньНедели,
	РАЗНОСТЬДАТ(НАЧАЛОПЕРИОДА(sms_Cheque.CloseTime, ДЕНЬ), sms_Cheque.CloseTime, МИНУТА) / &amp;ПериодВремени КАК IntervalTime,
	cl.Количество
ИЗ
	ВнешнийИсточникДанных.SIS.Таблица.sms_Cheque КАК sms_Cheque
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВнешнийИсточникДанных.SIS.Таблица.sms_Shift КАК sms_Shift
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВнешнийИсточникДанных.SIS.Таблица.sms_POS КАК sms_POS
				ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВнешнийИсточникДанных.SIS.Таблица.sms_Store КАК sms_Store
				ПО sms_POS.StoreUID = sms_Store.Ссылка
			ПО sms_Shift.PosUID = sms_POS.Ссылка
		ПО sms_Cheque.ShiftUID = sms_Shift.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ ВнешнийИсточникДанных.SIS.Таблица.sms_Employee КАК sms_Employee
		ПО sms_Cheque.CashierUID = sms_Employee.Ссылка
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ (ВЫБРАТЬ
			КОЛИЧЕСТВО(*) КАК ChequeLineCount,
			СУММА(t.Количество) КАК Количество,
			t.ChequeUID КАК ChequeUID
		ИЗ
			(ВЫБРАТЬ
				sms_ChequeLine.SaleItemUID КАК ChequeLineCount,
				СУММА(sms_ChequeLine.Quantity) КАК Количество,
				sms_Cheque.ChequeUID КАК ChequeUID
			ИЗ
				ВнешнийИсточникДанных.SIS.Таблица.sms_ChequeLine КАК sms_ChequeLine
					ЛЕВОЕ СОЕДИНЕНИЕ ВнешнийИсточникДанных.SIS.Таблица.sms_Cheque КАК sms_Cheque
					ПО sms_ChequeLine.ChequeUID = sms_Cheque.Ссылка
			
			СГРУППИРОВАТЬ ПО
				sms_Cheque.ChequeUID,
				sms_ChequeLine.SaleItemUID) КАК t
		ГДЕ
			t.Количество &gt; 0
		
		СГРУППИРОВАТЬ ПО
			t.ChequeUID) КАК cl
		ПО (cl.ChequeUID = sms_Cheque.ChequeUID)
ГДЕ
	sms_Cheque.CloseTime МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И ВЫБОР ДЕНЬНЕДЕЛИ(&amp;ПервоеЧисло2001)
			КОГДА 2
				ТОГДА ВЫБОР
						КОГДА ДЕНЬНЕДЕЛИ(sms_Cheque.OpenTime) = 1
							ТОГДА 7
						ИНАЧЕ ДЕНЬНЕДЕЛИ(sms_Cheque.OpenTime) - 1
					КОНЕЦ
			ИНАЧЕ ДЕНЬНЕДЕЛИ(sms_Cheque.OpenTime)
		КОНЕЦ В (&amp;ДниНедели)
	И sms_Shift.CloseTime ЕСТЬ НЕ NULL 
	И sms_Cheque.CloseType = 0
{ГДЕ
	(ВЫБОР
			КОГДА sms_Cheque.ChequeType = 0
				ТОГДА "Продажа"
			КОГДА sms_Cheque.ChequeType = 1
				ТОГДА "Возврат"
			ИНАЧЕ "Не известен"
		КОНЕЦ) КАК Тип}</query>
