			<dataPath>LOC</dataPath>
			<field>LOC</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>LOC_TYPE</dataPath>
			<field>LOC_TYPE</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Магазин</dataPath>
			<field>Магазин</field>
		</field>
		<field xsi:type="DataSetFieldField">
