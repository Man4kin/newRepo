			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	СправочникНоменклатура.Ссылка КАК Номенклатура,
	СправочникНоменклатура.Код,
	ВЫБОР
		КОГДА СправочникНоменклатура.ПометкаУдаления
			ТОГДА ВЫБОР
					КОГДА СправочникНоменклатура.Группа = 0
						ТОГДА 2
					ИНАЧЕ 3
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА СправочникНоменклатура.Группа = 0
					ТОГДА 0
				ИНАЧЕ 1
			КОНЕЦ
	КОНЕЦ КАК Картинка,
	ПараметрыЗаказов.СтраховойЗапас,
	ПараметрыЗаказов.МаксимальноеОтклонение,
	ПараметрыЗаказов.МинимальноеОтклонение
ИЗ
	Справочник.Номенклатура КАК СправочникНоменклатура
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ПараметрыЗаказов КАК ПараметрыЗаказов
		ПО (ПараметрыЗаказов.Номенклатура = СправочникНоменклатура.Ссылка)
			И (ПараметрыЗаказов.СтруктурнаяЕдиница В ИЕРАРХИИ (&amp;СтруктурнаяЕдиница))
ГДЕ
	(СправочникНоменклатура.Группа = 1
			ИЛИ СправочникНоменклатура.Транзакционный)</QueryText>
