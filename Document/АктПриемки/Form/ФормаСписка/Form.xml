			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументАктПриемки.ПометкаУдаления,
	ДокументАктПриемки.Номер,
	ДокументАктПриемки.Дата,
	ДокументАктПриемки.Проведен,
	ДокументАктПриемки.Автор,
	ДокументАктПриемки.Валюта,
	ДокументАктПриемки.Грузоотправитель,
	ДокументАктПриемки.ДатаДокумента,
	ВЫРАЗИТЬ(ДокументАктПриемки.ДокОснование КАК Документ.ПланируемаяПоставка).Номер КАК НомерДокументаОснования,
	ДокументАктПриемки.Заключение,
	ДокументАктПриемки.Кладовщик,
	ДокументАктПриемки.Комментарий,
	ДокументАктПриемки.Контрагент,
	ДокументАктПриемки.Кратность,
	ДокументАктПриемки.Курс,
	ДокументАктПриемки.НомерДокумента,
	ДокументАктПриемки.ПредстСотр,
	ДокументАктПриемки.ПредстФирма,
	ДокументАктПриемки.Склад,
	ДокументАктПриемки.СтруктурнаяЕдиница,
	ДокументАктПриемки.ТипДоговора,
	ДокументАктПриемки.Товаровед,
	ДокументАктПриемки.Фирма,
	ДокументАктПриемки.Ссылка,
	ДокументАктПриемки.ВсегоПоДокументу,
	ДокументАктПриемки.ВсегоПоФакту,
	ДокументАктПриемки.ВсегоРасхождение,
	ВЫБОР
		КОГДА НЕ Izh_СтатусEDIСообщений.Документ ЕСТЬ NULL 
			ТОГДА "Загружен DESADV"
		КОГДА ТИПЗНАЧЕНИЯ(ДокументАктПриемки.ДокОснование) = ТИП(Документ.ПланируемаяПоставка)
				И ДокументАктПриемки.ДокОснование &lt;&gt; ЗНАЧЕНИЕ(Документ.ПланируемаяПоставка.ПустаяСсылка)
			ТОГДА "На основании ПП оператором"
		ИНАЧЕ "Оператором без основания"
	КОНЕЦ КАК Статус
ИЗ
	Документ.АктПриемки КАК ДокументАктПриемки
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.Izh_СтатусEDIСообщений КАК Izh_СтатусEDIСообщений
		ПО ДокументАктПриемки.Ссылка = Izh_СтатусEDIСообщений.Документ
			И (Izh_СтатусEDIСообщений.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыEDIСообщений.Получен))</QueryText>
				<MainTable>Document.АктПриемки</MainTable>
