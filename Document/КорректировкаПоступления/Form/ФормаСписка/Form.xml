			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументКорректировкаПоступления.Ссылка,
	ДокументКорректировкаПоступления.ВерсияДанных,
